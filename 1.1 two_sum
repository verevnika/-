class Solution:
    def twoSum(self, nums, target):
        numbers_seen = {}  
        
        for index in range(len(nums)):  
            current_number = nums[index]  
            difference = target - current_number  
            
            if difference in numbers_seen:  
                return [numbers_seen[difference], index]  
            
            numbers_seen[current_number] = index
